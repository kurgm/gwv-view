{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "indent": [true, "tabs"],
      "max-classes-per-file": false,
      "no-bitwise": false,
      "variable-name": {
        "options": [
          "ban-keywords",
          "check-format",
          "allow-leading-underscore",
          "allow-pascal-case"
        ]
      }
    },
    "rulesDirectory": []
}
